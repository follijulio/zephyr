generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  email    String @unique
  password String
}

model Note {
  id      String  @id @default(auto()) @map("_id") @db.ObjectId
  note    String
  title   String
  actived Boolean
  @@map("Note")
}
